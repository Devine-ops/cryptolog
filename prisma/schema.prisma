generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id              Int      @id @default(autoincrement())
  name            String
  email           String   @unique
  password        String
  dateOfBirth     DateTime
  nationality     String
  phone           String
  typeOfDocument  Json
  documentNumber  String
  imgDocument     Json
  completeAddress String
  postalCode      String
  state           String
  country         String
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}
